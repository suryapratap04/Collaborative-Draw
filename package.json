{
  "name": "CollaborativeDraw",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./packages/*/dist ./apps/*/dist ./node_modules/.prisma && rm -rf node_modules && rm -rf pnpm-lock.yaml",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20",
    "pnpm": "9.0.0"
  }
}
